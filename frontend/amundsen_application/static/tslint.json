{
    "defaultSeverity": "error",
    "extends": [],
    "jsRules": {
      "no-empty": true
    },
    "rules": {
      "variable-name": [
         true,
         "ban-keywords",
         "check-format",
         "allow-pascal-case",
         "allow-leading-underscore"
       ],
       "import-name": [
         false
       ],
       "no-parameter-reassignment": false,
       "prefer-array-literal": [
         false
       ],
       "prefer-template": false
    },
    "rulesDirectory": []
}
